{
  "name": "nestjs-project-structure",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project structure",
  "main": "dist/app",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "entity:load": "ts-node bin/entity",
    "entity:sync": "typeorm-ts-node-commonjs -d bin/ormconfig.ts schema:sync",
    "build": "nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:repl": "nest start --entryFile repl",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.ts -i",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "express-session": "^1.18.0",
    "graphql": "^16.8.1",
    "helmet": "^7.1.0",
    "mysql2": "^3.9.7",
    "nanoid": "^3.3.6",
    "nestjs-pino": "^4.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.1.0",
    "pino-http": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.24",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/testing": "^10.3.8",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/prompts": "^2.4.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "esbuild": "^0.21.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^0.25.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typeorm-model-generator": "^0.4.6-no-engines",
    "typescript": "~5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/nestjs-project-structure.git"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://github.com/CatsMiaow/nestjs-project-structure#readme",
  "author": "CatsMiaow",
  "license": "MIT"
}
